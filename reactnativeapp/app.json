{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "reactnativeapp",
    "slug": "reactnativeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcon_ios.png",
    "android.icon": "./assets/ic_launcher_foreground.png",
    "android.adaptiveIcon.foregroundImage": "./assets/ic_launcher_foreground.png",
    "android.adaptiveIcon.backgroundColor": "#5C89D9",
    "ios.icon": "./assets/AppIcon_ios.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/d407db70-b6e0-4d0e-a1de-73959b8b00e4"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.filipacchi.reactnativeapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d407db70-b6e0-4d0e-a1de-73959b8b00e4"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
